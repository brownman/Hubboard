<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">	<object class="wxFrame" name="main_frame" subclass="MainFrame">		<style>wxDEFAULT_FRAME_STYLE|wxSYSTEM_MENU|wxTAB_TRAVERSAL</style>		<size>500,300</size>		<title>Hubboard</title>		<centered>1</centered>		<object class="wxBoxSizer">			<orient>wxVERTICAL</orient>			<object class="sizeritem">				<option>0</option>				<flag>wxEXPAND | wxALL</flag>				<border>5</border>				<object class="wxPanel" name="user_panel">					<style>wxTAB_TRAVERSAL</style>					<object class="wxBoxSizer">						<orient>wxHORIZONTAL</orient>						<object class="sizeritem">							<option>0</option>							<flag>wxEXPAND</flag>							<border>5</border>							<object class="wxBoxSizer">								<orient>wxVERTICAL</orient>								<object class="sizeritem">									<option>0</option>									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL</flag>									<border>5</border>									<object class="wxStaticText" name="user_name">										<label>MyLabel</label>										<wrap>-1</wrap>									</object>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALL</flag>									<border>5</border>									<object class="wxStaticBitmap" name="user_avatar">										<size>80,80</size>										<bitmap>../assets/nn_standard.png</bitmap>									</object>								</object>							</object>						</object>						<object class="sizeritem">							<option>1</option>							<flag>wxALL|wxEXPAND</flag>							<border>5</border>							<object class="wxHtmlWindow" name="details_html">								<style>wxHW_SCROLLBAR_AUTO</style>							</object>						</object>					</object>				</object>			</object>			<object class="sizeritem">				<option>1</option>				<flag>wxEXPAND | wxALL</flag>				<border>5</border>				<object class="wxScrolledWindow" name="timeline_scroller">					<style>wxFULL_REPAINT_ON_RESIZE|wxVSCROLL</style>					<object class="wxBoxSizer">						<orient>wxVERTICAL</orient>					</object>				</object>			</object>		</object>	</object>	<object class="wxPanel" name="event_panel" subclass="EventPanel">		<style>wxTAB_TRAVERSAL</style>		<size>330,150</size>		<object class="wxBoxSizer">			<orient>wxVERTICAL</orient>			<object class="sizeritem">				<option>0</option>				<flag>wxEXPAND</flag>				<border>5</border>				<object class="wxBoxSizer">					<orient>wxHORIZONTAL</orient>					<object class="sizeritem">						<option>0</option>						<flag>wxALL|wxEXPAND</flag>						<border>5</border>						<object class="wxStaticBitmap" name="user_avatar">							<size>40,40</size>							<bitmap>../assets/nn_standard.png</bitmap>						</object>					</object>					<object class="sizeritem">						<option>1</option>						<flag>wxEXPAND</flag>						<border>5</border>						<object class="wxBoxSizer">							<orient>wxVERTICAL</orient>							<object class="sizeritem">								<option>0</option>								<flag>wxALL|wxEXPAND</flag>								<border>5</border>								<object class="wxHyperlinkCtrl" name="username_link">									<label>username</label>									<url>http://www.wxformbuilder.org</url>									<style>wxHL_ALIGN_LEFT</style>								</object>							</object>							<object class="sizeritem">								<option>1</option>								<flag>wxALL|wxEXPAND</flag>								<border>5</border>								<object class="wxStaticText" name="title_label">									<label>Title</label>									<wrap>-1</wrap>								</object>							</object>						</object>					</object>				</object>			</object>			<object class="sizeritem">				<option>1</option>				<flag>wxALL|wxEXPAND</flag>				<border>5</border>				<object class="wxHtmlWindow" name="contents_html">					<style>wxHW_NO_SELECTION|wxHW_SCROLLBAR_AUTO</style>					<size>-1,100</size>					<font>						<size>7</size>						<family>default</family>						<style>normal</style>						<weight>normal</weight>						<underlined>0</underlined>					</font>				</object>			</object>		</object>	</object></resource>